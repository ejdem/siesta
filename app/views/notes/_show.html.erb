<div class="col-md-8 col-md-offset-2">    
  <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>Course id</th>
          <th>Course name</th>
          <th>Notes</th>
        </tr>
      </thead>
      <tbody>
        <% @subject_ids.each do |s_id| %>
          <%  subject = Subject.find(s_id) %>
          <%  notes = [] %>
          <%  notes = subject.notes.where(user_id: @user.id) %>
          <tr>
              <td><%= s_id %></td>
              <td><%= subject.name %></td>
              <td><% notes.each do |n| %> <%= n.note %> <% end %></td>
          </tr>
        <% end %>
      </tbody>
  </table> 
</div>