<p>Your Courses</p>
<div class="col-md-6 col-offset-3">
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>#</th>
      <th>Course name</th>
    </tr>
  </thead>
  <tbody>
    <% @user.active_participations.each do |particip| %>
      <% s_id = particip.subject_id %>
      <% s = Subject.find(s_id) %>
      <tr>
          <td><%= s_id %></td>
          <td><%= link_to s.name, s %></td>
      </tr>
    <% end %>
  </tbody>
</table> 
</div>