<% provide(:title, @user.name) %>
<div class="row">
    <div class="col-md-6 col-md-offset-3">
      <% if !@user.tutor? %>
        <%= render 'participations' %>
      <% else %>
        <p>good moring mister <%= @user.name %></p>
      <% end %>
    </div>
</div>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <% if !@user.tutor? && !@user.admin? %>
      <%= render 'notes/show' %>
    <% end %>
  </div>
</div>